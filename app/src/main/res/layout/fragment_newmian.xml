      android:layout_marginLeft="226dp"
      android:textColor="#ffffffff"
      android:textSize="40sp"
      />
  <ImageView  app:layout_constraintLeft_toLeftOf="@+id/view_jieshao"
      app:layout_constraintTop_toTopOf="@+id/view_jieshao"
      app:layout_constraintBottom_toBottomOf="@+id/view_jieshao"
      android:layout_marginLeft="42dp"
      android:layout_width="138dp"
      android:layout_height="138dp"
      android:src="@mipmap/guanku"/>
