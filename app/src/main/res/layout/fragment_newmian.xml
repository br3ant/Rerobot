      android:layout_marginLeft="42dp"
      android:layout_width="138dp"
      android:layout_height="138dp"
      android:src="@mipmap/daolang"/>
  <TextView
      app:layout_constraintLeft_toLeftOf="@+id/view_set"
      app:layout_constraintTop_toTopOf="@+id/view_set"
      app:layout_constraintBottom_toBottomOf="@+id/view_set"
